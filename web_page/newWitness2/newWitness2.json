    },
    "filteredList" : {
      "type" : "expression",
      "value" : "var lista = [];\r\nif($data.userlist && $data.m){\r\n    for(var i =0; i< $data.userlist.length; i++){\r\n        if($data.userlist[i].id != $data.m.mandatorId && $data.userlist[i].id != $data.m.mandatedId && $data.userlist[i].id != $data.m.witness1id && $data.userlist[i].id != $data.m.temp){\r\n            lista.push({name: $data.userlist[i].lastName + \", \" + $data.userlist[i].firstName, id: $data.userlist[i].id});\r\n        }\r\n    }\r\n}\r\nreturn lista;",
