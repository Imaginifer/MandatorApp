    },
    "filteredList" : {
      "type" : "expression",
      "value" : "var lista = [];\nif($data.userlist && $data.m){\n   for (var i = 0; i< $data.userlist.length; i++){\n        if($data.userlist[i].id != $data.m.mandatorId ){\n            lista.push({name: $data.userlist[i].lastName + \", \" + $data.userlist[i].firstName, id: $data.userlist[i].id});\n        }\n    }\n}\n\nreturn lista;",
      "exposed" : false
