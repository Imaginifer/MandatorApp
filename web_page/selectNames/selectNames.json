      "type" : "expression",
      "value" : "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){\n\tconst liElements = $data.formOutput._submitError.explanations\n\t\t.filter(cause => cause !== null)\n\t\t.map(cause => \"<li>\" + cause + \"</li>\")\n\t\t.join('');\n\tif(liElements){\n\t\treturn \"<ul>\" + liElements + \"</ul>\";\n\t}\n}",
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : "if( $data.m ){\n\treturn {\n\t\t//map m variable to expected task contract input\n\t\tmInput: {\n\t\t\tmandatedId: $data.m.mandatedId !== undefined ? $data.m.mandatedId : null,\n\t\t\twitness1id: $data.m.witness1id !== undefined ? $data.m.witness1id : null,\n\t\t\twitness2id: $data.m.witness2id !== undefined ? $data.m.witness2id : null\n\t\t}\n\t}\n}",
      "exposed" : false
    },
    "context" : {
