    "formOutput" : {
      "type" : "expression",
      "value" : "if( $data.m ){\n\treturn {\n\t\t//map m variable to expected task contract input\n\t\tmInput: {\n\t\t\tmandatedId: $data.m.mandatedId !== undefined ? $data.m.mandatedId : null\n\t\t}\n\t}\n}",
